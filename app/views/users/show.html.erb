<a href="/songs">Dashboard</a>
<h1><%= @user.first_name %> <%= @user.last_name %>'s Playlist</h1>
<table>
  <tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Times Added</th>
  </tr>
  <tr>
    <% @list.each do |list| %>
    <td><%= list.artist %></td>
    <td><%= list.title %></td>
    <td><%= list.songs_count %></td>
  </tr>
  <% end %>
</table>
